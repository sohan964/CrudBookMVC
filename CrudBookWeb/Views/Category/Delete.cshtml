<form method="post" asp-action="DeletePOST" >
    <input asp-for="Id" hidden />
    <div class="border p-3 mt-4">
        <div class=" row pb-2">
            <h2 class="text-primary">Delete Category</h2>
        </div>
        <div class="mb-3">
            <label asp-for="Name"></label>
            <input asp-for="Name" disabled class="form-control"  />
            
        </div>
        <div class="mb-3">
            <label asp-for="DisplayOrder"></label>
            <input asp-for="DisplayOrder" disabled class="form-control" />
            
        </div>
        <input class="btn btn-danger" type="submit" value="Delete"/>
        <a asp-controller="Category" asp-action="Index" class="btn btn-outline-primary">Go Back</a>
    </div>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}